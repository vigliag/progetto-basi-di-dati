<table class="table">
  <thead>
    <tr>
      <th>Starts</th>
      <th>Movie</th>
      <th>Price</th>
      <th>Screen</th>
      <th>Tickets Sold</th>
      <th>Avaiable seats</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shows.each do |show| %>
      <tr>
        <td><%= show.start.to_s(:time) %></td>
        <td><%= number_to_currency show.price %></td>
        <td><%= show.screen_id %></td>
        <td><%= show.ticket_count %> </td>
        <td><%= show.avaiable_seats.length %></td>
        <td>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>